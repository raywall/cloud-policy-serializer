{
	"PkgHandler": {
		"Pattern": "/cloud-policy-serializer",
		"StripPrefix": "/cloud-policy-serializer",
		"Include": [
			"/src/github.com/raywall/cloud-policy-serializer/cmd",
			"/src/github.com/raywall/cloud-policy-serializer/examples",
			"/src/github.com/raywall/cloud-policy-serializer/pkg"
		],
		"Exclude": [
			"/src/github.com/raywall/cloud-policy-serializer/sam",
			"/src/github.com/raywall/cloud-policy-serializer/.aws-sam",
			"/src/github.com/raywall/cloud-policy-serializer/*.sh",
			"/src/github.com/raywall/cloud-policy-serializer/Makefile",
			"/src/github.com/raywall/cloud-policy-serializer/go.*"
		],
		"IgnoredSuffixes": [
			"iface"
		]
	},
	"Github": {
		"Tag": "main",
		"Repo": "/raywall/cloud-policy-serializer",
		"UseGithub": true
	}
}